# Development environment (for testing only)
# Copy to .env and adjust values for local development
NODE_ENV=development
DB_PATH=./dev.db
# Email (Ethereal fallback used if not set)
#SMTP_HOST=
#SMTP_PORT=
#SMTP_USER=
#SMTP_PASS=
#SMTP_SECURE=false
ALERT_FROM=no-reply@daftar.local
ALERT_TO=admin@example.com
# WhatsApp admin number (local outbox used if Twilio not configured)
ADMIN_WHATSAPP=779992669
# Backups are disabled by default in dev
ENABLE_BACKUPS=false
ENABLE_SCHEDULERS=false
BACKUP_ENCRYPT=false
# AWS S3 (optional)
#BACKUP_S3_BUCKET=
#BACKUP_S3_KEY=
#BACKUP_S3_SECRET=
#BACKUP_S3_REGION=
#BACKUP_S3_ENDPOINT=
